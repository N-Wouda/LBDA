#include <iostream>

#include "gurobi_c++.h"

using namespace std;

int main(int argc, char *argv[])
{  
  GRBEnv env;  
  // env.set(GRB_IntParam_OutputFlag, 0); 
  env.set(GRB_IntParam_Threads, 1);
  
  GRBModel model(env);
  
  GRBVar x = model.addVar(0, 1, -1, GRB_CONTINUOUS, "x");
  model.optimize();
  
  cout << "basis: " << x.get(GRB_IntAttr_VBasis) << '\n';
  
}